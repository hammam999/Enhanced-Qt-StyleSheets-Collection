QToolBox {
	color: #a9b7c6;
	background-color:#000000;
}
QToolBox::tab {
	color: #a9b7c6;
	background-color:#000000;
}
QToolBox::tab:selected {
	color: #FFFFFF;
	background-color:#000000;
}
QScrollArea {
	color: #FFFFFF;
	background-color:#000000;
}
QSlider::groove:horizontal {
	height: 5px;
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(49, 147, 250, 255), stop:1 rgba(34, 142, 255, 255));
}
QSlider::groove:vertical {
	width: 5px;
	background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(49, 147, 250, 255), stop:1 rgba(34, 142, 255, 255));
}
QSlider::handle:horizontal {
	background: rgb(253,253,253);
	border-style: solid;
	border-width: 1px;
	border-color: rgb(207,207,207);
	width: 12px;
	margin: -5px 0;
	border-radius: 7px;
}
QSlider::handle:vertical {
	background: rgb(253,253,253);
	border-style: solid;
	border-width: 1px;
	border-color: rgb(207,207,207);
	height: 12px;
	margin: 0 -5px;
	border-radius: 7px;
}
QSlider::add-page:horizontal {
    background: rgb(181,181,181);
}
QSlider::add-page:vertical {
    background: rgb(181,181,181);
}
QSlider::sub-page:horizontal {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(49, 147, 250, 255), stop:1 rgba(34, 142, 255, 255));
}
QSlider::sub-page:vertical {
    background-color: qlineargradient(spread:pad, y1:0.5, x1:1, y2:0.5, x2:0, stop:0 rgba(49, 147, 250, 255), stop:1 rgba(34, 142, 255, 255));
}
QScrollBar:horizontal {
	max-height: 20px;
	border: 1px transparent grey;
	margin: 0px 20px 0px 20px;
}
QScrollBar:vertical {
	max-width: 20px;
	border: 1px transparent grey;
	margin: 20px 0px 20px 0px;
}
QScrollBar::handle:horizontal {
	background: rgb(253,253,253);
	border-style: solid;
	border-width: 1px;
	border-color: rgb(207,207,207);
	border-radius: 7px;
	min-width: 25px;
}
QScrollBar::handle:horizontal:hover {
	background: rgb(253,253,253);
	border-style: solid;
	border-width: 1px;
	border-color: rgb(147, 200, 200);
	border-radius: 7px;
	min-width: 25px;
}
QScrollBar::handle:vertical {
	background: rgb(253,253,253);
	border-style: solid;
	border-width: 1px;
	border-color: rgb(207,207,207);
	border-radius: 7px;
	min-height: 25px;
}
QScrollBar::handle:vertical:hover {
	background: rgb(253,253,253);
	border-style: solid;
	border-width: 1px;
	border-color: rgb(147, 200, 200);
	border-radius: 7px;
	min-height: 25px;
}
QScrollBar::add-line:horizontal {
   border: 2px transparent grey;
   border-top-right-radius: 7px;
   border-bottom-right-radius: 7px;
   background: rgba(34, 142, 255, 255);
   width: 20px;
   subcontrol-position: right;
   subcontrol-origin: margin;
}
QScrollBar::add-line:horizontal:pressed {
   border: 2px transparent grey;
   border-top-right-radius: 7px;
   border-bottom-right-radius: 7px;
   background: rgb(181,181,181);
   width: 20px;
   subcontrol-position: right;
   subcontrol-origin: margin;
}
QScrollBar::add-line:vertical {
   border: 2px transparent grey;
   border-bottom-left-radius: 7px;
   border-bottom-right-radius: 7px;
   background: rgba(34, 142, 255, 255);
   height: 20px;
   subcontrol-position: bottom;
   subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:pressed {
   border: 2px transparent grey;
   border-bottom-left-radius: 7px;
   border-bottom-right-radius: 7px;
   background: rgb(181,181,181);
   height: 20px;
   subcontrol-position: bottom;
   subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
   border: 2px transparent grey;
   border-top-left-radius: 7px;
   border-bottom-left-radius: 7px;
   background: rgba(34, 142, 255, 255);
   width: 20px;
   subcontrol-position: left;
   subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal:pressed {
   border: 2px transparent grey;
   border-top-left-radius: 7px;
   border-bottom-left-radius: 7px;
   background: rgb(181,181,181);
   width: 20px;
   subcontrol-position: left;
   subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
   border: 2px transparent grey;
   border-top-left-radius: 7px;
   border-top-right-radius: 7px;
   background: rgba(34, 142, 255, 255);
   height: 20px;
   subcontrol-position: top;
   subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical:pressed {
   border: 2px transparent grey;
   border-top-left-radius: 7px;
   border-top-right-radius: 7px;
   background: rgb(181,181,181);
   height: 20px;
   subcontrol-position: top;
   subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal {
   border: 1px transparent grey;
   border-top-left-radius: 3px;
   border-bottom-left-radius: 3px;
   width: 6px;
   height: 6px;
   background: white;
}
QScrollBar::right-arrow:horizontal {
   border: 1px transparent grey;
   border-top-right-radius: 3px;
   border-bottom-right-radius: 3px;
   width: 6px;
   height: 6px;
   background: white;
}
QScrollBar::up-arrow:vertical {
   border: 1px transparent grey;
   border-top-left-radius: 3px;
   border-top-right-radius: 3px;
   width: 6px;
   height: 6px;
   background: white;
}
QScrollBar::down-arrow:vertical {
   border: 1px transparent grey;
   border-bottom-left-radius: 3px;
   border-bottom-right-radius: 3px;
   width: 6px;
   height: 6px;
   background: white;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
   background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
   background: none;
}

/* Enhanced Components - Modern Aqua Extensions */

/* ComboBox Styling */
QComboBox {
    border: 1px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 5px;
    padding: 4px 8px;
    background-color: rgb(255, 255, 255);
    color: #000000;
    min-width: 80px;
}

QComboBox:hover {
    border-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(49, 147, 250, 255), stop:1 rgba(34, 142, 255, 255));
}

QComboBox:focus {
    border-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    outline: none;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid rgba(0, 113, 255, 100);
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(240, 240, 240, 255), stop:1 rgba(255, 255, 255, 255));
}

QComboBox::down-arrow {
    image: url(icons/svg/dropdown.svg);
    width: 12px;
    height: 12px;
}

QComboBox QAbstractItemView {
    border: 1px solid rgba(0, 113, 255, 255);
    border-radius: 5px;
    background-color: rgb(255, 255, 255);
    selection-background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    selection-color: white;
    outline: none;
}

/* TreeView and ListView */
QTreeView, QListView {
    border: 1px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 5px;
    background-color: rgb(255, 255, 255);
    alternate-background-color: rgb(248, 248, 248);
    selection-background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    selection-color: white;
    outline: none;
}

QTreeView::item, QListView::item {
    padding: 4px;
    border: none;
}

QTreeView::item:hover, QListView::item:hover {
    background-color: rgba(0, 113, 255, 50);
    border-radius: 3px;
}

QTreeView::item:selected, QListView::item:selected {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    color: white;
    border-radius: 3px;
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(icons/svg/arrow-right.svg) 0;
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(icons/svg/arrow-right.svg) 0;
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(icons/svg/arrow-right.svg) 0;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(icons/svg/arrow-right.svg);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings {
    border-image: none;
    image: url(icons/svg/arrow-down.svg);
}

/* TableView */
QTableView {
    border: 1px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 5px;
    background-color: rgb(255, 255, 255);
    alternate-background-color: rgb(248, 248, 248);
    selection-background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    selection-color: white;
    gridline-color: rgba(0, 113, 255, 100);
    outline: none;
}

QTableView::item {
    padding: 4px;
    border: none;
}

QTableView::item:hover {
    background-color: rgba(0, 113, 255, 50);
}

QTableView::item:selected {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    color: white;
}

/* HeaderView */
QHeaderView::section {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(240, 240, 240, 255), stop:1 rgba(255, 255, 255, 255));
    border: 1px solid rgba(0, 113, 255, 100);
    border-radius: 0px;
    padding: 6px;
    font-weight: bold;
    color: #000000;
}

QHeaderView::section:hover {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(230, 240, 255, 255), stop:1 rgba(245, 250, 255, 255));
}

QHeaderView::section:pressed {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    color: white;
}

/* Splitter */
QSplitter::handle {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(220, 220, 220, 255), stop:1 rgba(240, 240, 240, 255));
    border: 1px solid rgba(0, 113, 255, 100);
}

QSplitter::handle:horizontal {
    width: 6px;
    border-radius: 3px;
}

QSplitter::handle:vertical {
    height: 6px;
    border-radius: 3px;
}

QSplitter::handle:hover {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
}

/* GroupBox */
QGroupBox {
    font-weight: bold;
    border: 2px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 8px;
    margin-top: 10px;
    padding-top: 10px;
    background-color: rgba(255, 255, 255, 200);
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 8px 0 8px;
    background-color: rgb(255, 255, 255);
    color: rgba(0, 113, 255, 255);
    border-radius: 4px;
}

/* Frame */
QFrame {
    border: 1px solid rgba(0, 113, 255, 100);
    border-radius: 5px;
    background-color: rgb(255, 255, 255);
}

QFrame[frameShape="1"] { /* Box */
    border: 2px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
}

QFrame[frameShape="2"] { /* Panel */
    border: 1px solid rgba(0, 113, 255, 150);
    background-color: rgba(255, 255, 255, 240);
}

/* DockWidget */
QDockWidget {
    border: 1px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 5px;
    background-color: rgb(255, 255, 255);
}

QDockWidget::title {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    color: white;
    padding: 6px;
    font-weight: bold;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

QDockWidget::close-button, QDockWidget::float-button {
    background-color: transparent;
    border: 1px solid transparent;
    border-radius: 3px;
    padding: 2px;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background-color: rgba(255, 255, 255, 100);
    border: 1px solid rgba(255, 255, 255, 150);
}

/* Calendar Widget */
QCalendarWidget {
    border: 1px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 8px;
    background-color: rgb(255, 255, 255);
}

QCalendarWidget QToolButton {
    background-color: transparent;
    border: none;
    color: rgba(0, 113, 255, 255);
    font-weight: bold;
}

QCalendarWidget QToolButton:hover {
    background-color: rgba(0, 113, 255, 50);
    border-radius: 4px;
}

QCalendarWidget QAbstractItemView {
    background-color: rgb(255, 255, 255);
    selection-background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    selection-color: white;
}

/* ToolTip */
QToolTip {
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    color: white;
    border: 1px solid rgba(0, 113, 255, 255);
    border-radius: 6px;
    padding: 6px;
    font-size: 12px;
}

/* Dialog */
QDialog {
    background-color: rgb(236, 236, 236);
    border: 2px solid qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 10px;
}

/* Enhanced CheckBox with SVG Icons */
QCheckBox::indicator:checked {
    image: url(icons/svg/checkbox-checked.svg);
    height: 16px;
    width: 16px;
    border-style: solid;
    border-width: 1px;
    border-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 3px;
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
}

QCheckBox::indicator:unchecked {
    image: url(icons/svg/checkbox-unchecked.svg);
    height: 16px;
    width: 16px;
    border-style: solid;
    border-width: 1px;
    border-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    border-radius: 3px;
    background-color: rgb(255, 255, 255);
}

/* Enhanced RadioButton with SVG Icons */
QRadioButton::indicator:checked {
    image: url(icons/svg/radio-checked.svg);
    height: 16px;
    width: 16px;
    border-style: solid;
    border-radius: 8px;
    border-width: 1px;
    border-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
}

QRadioButton::indicator:!checked {
    image: url(icons/svg/radio-unchecked.svg);
    height: 16px;
    width: 16px;
    border-style: solid;
    border-radius: 8px;
    border-width: 1px;
    border-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(0, 113, 255, 255), stop:1 rgba(91, 171, 252, 255));
    background-color: rgb(255, 255, 255);
}

/* Enhanced Scroll Arrows with SVG Icons */
QScrollBar::left-arrow:horizontal {
    image: url(icons/svg/arrow-left.svg);
    width: 12px;
    height: 12px;
    background: white;
    border-radius: 3px;
}

QScrollBar::right-arrow:horizontal {
    image: url(icons/svg/arrow-right.svg);
    width: 12px;
    height: 12px;
    background: white;
    border-radius: 3px;
}

QScrollBar::up-arrow:vertical {
    image: url(icons/svg/arrow-up.svg);
    width: 12px;
    height: 12px;
    background: white;
    border-radius: 3px;
}

QScrollBar::down-arrow:vertical {
    image: url(icons/svg/arrow-down.svg);
    width: 12px;
    height: 12px;
    background: white;
    border-radius: 3px;
}

/* Focus and Disabled States */
QWidget:focus {
    outline: 2px solid rgba(0, 113, 255, 150);
    outline-radius: 4px;
}

QWidget:disabled {
    color: #808086;
    background-color: rgba(200, 200, 200, 100);
}

/* Modern Animations and Transitions */
QPushButton, QToolButton {
    transition: all 0.2s ease-in-out;
}

QComboBox, QLineEdit, QTextEdit, QPlainTextEdit {
    transition: border-color 0.2s ease-in-out;
}
